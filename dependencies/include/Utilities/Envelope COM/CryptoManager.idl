		[id(3), helpstring("method SetKey")] HRESULT SetKey([in] const BSTR bstrPublicKey);
	};

	[
		object,
		uuid(1BA6A0A9-425E-4A5C-BD7F-DA64FFBC2ABE),
		dual,
		helpstring("ICryptoManager2 Interface"),
		pointer_default(unique)
	]
	interface ICryptoManager2 : ICryptoManager
	{
		[id(4), helpstring("method ImportKey")] HRESULT ImportKey([in] const BSTR bstrPrivateKey);
	};